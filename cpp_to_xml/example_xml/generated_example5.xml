          </vertex>
          <vertex condition = "i == 0 and j == 2 and k == 2" type = "1">
            <in bsrc = "0::2::6" src = "0, 2, 0"></in>
            <in bsrc = "0::2::5" src = "0, 2, 0"></in>
          </vertex>
          <vertex condition = "i == 0 and j == 1 and k == 0" type = "1">
            <in bsrc = "0::2::5" src = "0, 2, 0"></in>
