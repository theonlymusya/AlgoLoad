            <in bsrc = "0::2::6" src = "0, 1, 1"></in>
          </vertex>
          <vertex condition = "i == 1 and j == 2 and k == 1" type = "1">
            <in bsrc = "0::2::6" src = "0, 2, 1"></in>
            <in bsrc = "0::2::5" src = "1, 2, 1"></in>
          </vertex>
          <vertex condition = "i == 1 and j == 2 and k == 2" type = "1">
            <in bsrc = "0::2::6" src = "1, 2, 1"></in>
            <in bsrc = "0::2::5" src = "1, 2, 1"></in>
          </vertex>
        </block>

      </block>

  </block>

