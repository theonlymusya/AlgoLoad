<algo>
  <block id = "0" dims = "2">
    <arg name = "i" val = "0..2"></arg>
    <arg name = "j" val = "0..2"></arg>
      
      <!-- denom -->
      <block id = "1" dims = "0">
        <vertex condition = "i == 0 and j == 2" type = "0"></vertex>
        <vertex condition = "i == 0 and j == 1" type = "0">
          <in bsrc = "0::2::5" src = "0, 2, 0"></in>
          <in bsrc = "0::2::5" src = "0, 2, 0"></in>
        </vertex>
        <vertex condition = "i == 1 and j == 2" type = "0">
          <in bsrc = "0::2::6" src = "0, 1, 1"></in>
          <in bsrc = "0::2::6" src = "0, 1, 1"></in>
          <in bsrc = "0::2::6" src = "0, 2, 1"></in>
          <in bsrc = "0::2::6" src = "0, 2, 1"></in>
